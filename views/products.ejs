<%- include('header') %>
<!-- Section-->
  <section class="page-section mb-0" > 
    <div class="container my-section">
      <h1><%= __('LISTADO SELECCIONADO') %></h1>
      <h3>User: <%= email %></h3>
      <% products.forEach( function(product) { %>
        <p><b>=================================</b></p>
        <% if (product.name) { %>
          <h3><%= product.name %></h3>
        <% } %>
        <% if (product.price) { %>
          <p><%= __('Precio') %>: <%= product.price %> â‚¬</p>
        <% } %>
        <% if(product.sale !== undefined) { %>
          <% if(product.sale) {%>
            <p><%= __('Tipo: Venta') %></p>
          <% } else { %>
            <p><%= __('Tipo: Compra') %></p>
          <% } %>
        <% } %>
        <% if (product.tags !== undefined) { %>
          <p><%= __('Tag') %>: <%= product.tags[0] %></p>
        <% } %>
        <% if (product.image) { %>
          <p><%= __('Image') %>: <%= product.image %></p>
        <% } %>
        <% if (product._id) { %>
          <p>ID: <%= product._id %></p>
        <% } %>
      <% }) %>
    </div>
  </section>
<%- include('footer') %>